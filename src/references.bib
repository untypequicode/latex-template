% Fichier de bibliographie pour le template LaTeX
% Ajoutez vos références ici en suivant les exemples ci-dessous

% Exemple d'article de journal
@article{example_article,
  author  = {Nom, Prénom and Autre, Auteur},
  title   = {Titre de l'article},
  journal = {Nom du Journal},
  year    = {2024},
  volume  = {42},
  number  = {3},
  pages   = {123--145},
  doi     = {10.1234/example.doi}
}

% Exemple de livre
@book{example_book,
  author    = {Auteur, Nom},
  title     = {Titre du Livre},
  publisher = {Maison d'Édition},
  year      = {2023},
  edition   = {2},
  address   = {Ville}
}

% Exemple de chapitre de livre
@incollection{example_chapter,
  author    = {Contributeur, Nom},
  title     = {Titre du Chapitre},
  booktitle = {Titre du Livre},
  editor    = {Éditeur, Nom},
  publisher = {Maison d'Édition},
  year      = {2023},
  pages     = {45--67}
}

% Exemple de site web
@online{example_website,
  author  = {Organisation},
  title   = {Titre de la Page Web},
  year    = {2024},
  url     = {https://example.com},
  urldate = {2024-11-30}
}

% Exemple de conférence
@inproceedings{example_conference,
  author    = {Chercheur, Nom},
  title     = {Titre de la Présentation},
  booktitle = {Nom de la Conférence},
  year      = {2024},
  pages     = {1--10},
  address   = {Ville, Pays}
}

% Exemple de thèse
@phdthesis{example_thesis,
  author  = {Doctorant, Nom},
  title   = {Titre de la Thèse},
  school  = {Université},
  year    = {2023},
  address = {Ville, Pays}
}
